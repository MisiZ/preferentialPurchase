/* pages/feedback/index.wxss */
page {
  background-color: #eeeeee;
}
.fb_main {
  padding: 20rpx;
  color: #666;
}
.fb_main .fd_tips {
  display: flex;
  flex-wrap: wrap;
}
.fb_main .fd_tips text {
  width: 30%;
  padding: 10rpx;
  text-align: center;
  background-color: #fff;
  margin: 20rpx 10rpx;
}
.fb_main .fd_content {
  background-color: #fff;
  margin-top: 20rpx;
}
.fb_main .fd_content textarea {
  padding: 10rpx;
}
.fb_main .fd_content .fd_tool {
  display: flex;
  flex-wrap: wrap;
  padding-bottom: 30rpx;
}
.fb_main .fd_content .fd_tool .tool_btn {
  margin: 0;
  margin-top: 20rpx;
  margin-left: 10rpx;
  width: 90rpx;
  height: 90rpx;
  font-size: 60rpx;
  padding: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #ccc;
}
.fb_main .fd_content .fd_tool .up_img_item {
  margin-top: 20rpx;
  margin-left: 20rpx;
}
.fb_main .from_btn {
  margin-top: 20rpx;
}
.fb_main .from_btn .from_btn {
  background-color: var(--themeColor);
  width: 30%;
  color: #fff;
  margin-right: 20rpx;
}
